%DataStructs()
{
    struct SurfaceData
    {
        half3 Albedo;
        half3 Emission;
        half Alpha;

        // Additional Surface Data
        %AdditionalSurfaceData
        // Additional Surface Data End
    };

    SurfaceData CreateSurfaceData()
    {
        SurfaceData o = (SurfaceData) 0;
        o.Albedo = 0.5;
        o.Alpha = 1.0;
        return o;
    }
}